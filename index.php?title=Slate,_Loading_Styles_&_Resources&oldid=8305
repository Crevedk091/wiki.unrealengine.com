<span class="kw2">protected</span><span class="sy4">:</span>
	<span class="coMULTI">/*Creates resources*/</span>
	TSharedRef<span class="sy1">&lt;</span><span class="kw2">class</span> FSlateGameResources<span class="sy1">&gt;</span> Create<span class="br0">&#40;</span><span class="br0">&#41;</span><span class="sy4">;</span>
