	FMyUIResources<span class="br0">&#40;</span><span class="br0">&#41;</span> <span class="sy4">:</span> m_Path<span class="br0">&#40;</span><span class="st0">&quot;/Game/UI&quot;</span><span class="br0">&#41;</span>, MyUIResources<span class="br0">&#40;</span><span class="kw2">NULL</span><span class="br0">&#41;</span> <span class="br0">&#123;</span> <span class="br0">&#125;</span> <span class="sy4">;</span>
&#160;
	<span class="coMULTI">/*Loads resources and registers them with Slate*/</span>
	<span class="coMULTI">/*Do this before trying to use resources*/</span>
	<span class="kw4">void</span> Initialize<span class="br0">&#40;</span><span class="br0">&#41;</span><span class="sy4">;</span>
&#160;
	<span class="coMULTI">/*cleanup*/</span>
	<span class="coMULTI">/*Do this when shutting down game module*/</span>
	<span class="kw4">void</span> Shutdown<span class="br0">&#40;</span><span class="br0">&#41;</span><span class="sy4">;</span>
&#160;
	<span class="coMULTI">/*reloads textures used by slate renderer*/</span>
	<span class="coMULTI">/*Does nothing at the moment*/</span>
	<span class="kw4">void</span> ReloadTextures<span class="br0">&#40;</span><span class="br0">&#41;</span><span class="sy4">;</span>
&#160;
	<span class="coMULTI">/*Give caller a pointer to our FSlateGameResources*/</span>
	TSharedPtr<span class="sy1">&lt;</span>FSlateGameResources<span class="sy1">&gt;</span> GetSlateGameResources<span class="br0">&#40;</span><span class="br0">&#41;</span><span class="sy4">;</span>
