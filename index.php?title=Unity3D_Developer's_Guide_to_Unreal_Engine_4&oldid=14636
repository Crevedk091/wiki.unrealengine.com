	ParticleSystem <span class="sy1">=</span> PCIP.<span class="me1">CreateDefaultSubobject</span><span class="sy1">&lt;</span>UParticleSystemComponent<span class="sy1">&gt;</span><span class="br0">&#40;</span><span class="kw3">this</span>, FName<span class="br0">&#40;</span>TEXT<span class="br0">&#40;</span><span class="st0">&quot;Particle System&quot;</span><span class="br0">&#41;</span><span class="br0">&#41;</span><span class="br0">&#41;</span><span class="sy4">;</span>
